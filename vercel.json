{
  "version": 2,
  "name": "uyo_creators_club",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "index.js": {
      "memory": 256,       
      "maxDuration": 10   
    }
  }
}
